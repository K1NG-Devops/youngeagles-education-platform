{
  "functions": {
    "app/**/*": {
      "runtime": "nodejs20.x"
    }
  },
  "framework": "vite",
  "buildCommand": "vite build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
