{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/app/(.*)",
      "destination": "/app/$1"
    },
    {
      "source": "/app",
      "destination": "/app/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
