{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "start:all": "./start-all.sh",
    "stop:all": "./stop-all.sh",
    "dev:pwa": "cd pwa && npm run dev",
    "build:pwa": "cd pwa && npm run build"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "turbo": "^2.5.2",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@9.2.0",
  "workspaces": [
    "api",
    "frontend",
    "pwa",
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "dotenv": "^16.5.0",
    "json2csv": "^6.0.0-alpha.2",
    "tailwindcss": "^4.1.10",
    "use-sound": "^5.0.0"
  }
}
